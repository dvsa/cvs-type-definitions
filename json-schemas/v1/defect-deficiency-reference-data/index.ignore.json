{
	"title": "Defect Deficiency Reference Data Schema",
	"type": "object",
	"properties": {
		"ref": {
			"type": "string"
		},
		"deficiencyId": {
			"type": [
				"string",
				"null"
			]
		},
		"deficiencySubId": {
			"type": "string"
		},
		"deficiencyCategory": {
			"type": "string"
		},
		"deficiencyText": {
			"type": "string"
		},
		"stdForProhibition": {
			"type": "boolean"
		},
		"forVehicleType": {
			"oneOf": [
				{
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				{
					"type": "string",
					"enum": [
						"psv",
						"hgv",
						"trl",
						"car",
						"lgv",
						"motorcycle"
					]
				}
			]
		}
	},
	"additionalProperties": false,
	"required": [
		"ref",
		"deficiencyId",
		"deficiencySubId",
		"deficiencyCategory",
		"deficiencyText",
		"stdForProhibition",
		"forVehicleType"
	]
}