			"type": "string"
		},
		"testStationPNumber": {
			"type": "string"
		},
		"testStationEmail": {
			"type": "string"
		},
		"testStationType": {
			"title": "Test Station Types",
			"type": "string",
			"tsEnumNames": [
				"ATF",
				"GVTS",
				"HQ",
				"POTF"
			],
			"enum": [
				"atf",
				"gvts",
				"hq",
				"potf"
			]
		},
		"testerName": {
			"type": "string"
		},
		"testerStaffId": {
			"type": "string"
		},
