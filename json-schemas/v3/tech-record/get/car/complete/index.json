		"vehicleSubclass": {
			"title": "Vehicle Subclass",
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"n",
					"p",
					"a",
					"s",
					"c",
					"l",
					"t",
					"e",
					"m",
