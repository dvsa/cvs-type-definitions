		"techRecord_vehicleConfiguration": {
			"title": "Vehicle Configuration",
			"type": "string",
			"enum": [
				"rigid",
				"articulated",
				"centre axle drawbar",
				"semi-car transporter",
				"semi-trailer",
				"long semi-trailer",
				"low loader",
				"other",
				"drawbar",
				"four-in-line",
				"dolly",
