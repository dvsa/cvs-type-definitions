{
	"title": "GET PSV Technical Record V3 Skeleton",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"vin",
		"systemNumber",
		"primaryVrm",
		"vehicleType",
		"techRecord_vehicleConfiguration",
		"techRecord_vehicleSize",
		"techRecord_seatsLowerDeck",
		"techRecord_seatsUpperDeck",
		"techRecord_numberOfWheelsDriven",
		"techRecord_vehicleType",
		"techRecord_noOfAxles",
		"techRecord_statusCode",
		"techRecord_reasonForCreation",
		"techRecord_vehicleClass_code",
		"techRecord_vehicleClass_description",
		"techRecord_createdAt"
	],
	"properties": {
		"vin": {
			"type": "string"
		},
		"systemNumber": {
			"type": "string"
		},
		"primaryVrm": {
			"type": "string"
		},
		"techRecord_vehicleType": {
			"const": "psv"
		},
		"techRecord_statusCode": {
			"title": "Status Code",
			"type": "string",
			"enum": [
				"provisional",
				"current",
				"archived"
			]
		},
		"techRecord_reasonForCreation": {
			"type": "string"
		},
		"techRecord_createdAt": {
			"type": "string"
		},
		"techRecord_vehicleConfiguration": {
			"title": "Vehicle Configuration",
			"type": "string",
			"enum": [
				"rigid",
				"articulated",
				"centre axle drawbar",
				"semi-car transporter",
				"semi-trailer",
				"long semi-trailer",
				"low loader",
				"other",
				"drawbar",
				"four-in-line",
				"dolly",
				"full drawbar"
			]
		},
		"techRecord_vehicleSize": {
			"title": "Vehicle Size",
			"type": "string",
			"enum": [
				"small",
				"large"
			]
		},
		"techRecord_seatsLowerDeck": {
			"type": "integer"
		},
		"techRecord_seatsUpperDeck": {
			"type": "integer"
		},
		"techRecord_numberOfWheelsDriven": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_vehicleClass_code": {
			"type": "string"
		},
		"techRecord_vehicleClass_description": {
			"title": "Vehicle Class Description",
			"type": "string",
			"enum": [
				"motorbikes over 200cc or with a sidecar",
				"not applicable",
				"small psv (ie: less than or equal to 22 seats)",
				"motorbikes up to 200cc",
				"trailer",
				"large psv(ie: greater than 23 seats)",
				"3 wheelers",
				"heavy goods vehicle",
				"MOT class 4",
				"MOT class 7",
				"MOT class 5"
			]
		},
		"techRecord_hiddenInVta": {
			"type": "boolean"
		},
		"techRecord_recordCompleteness": {
			"type": "string"
		},
		"techRecord_euVehicleCategory": {
			"anyOf": [
				{
					"title": "EU vehicle category",
					"type": "string",
					"enum": [
						"m1",
						"m2",
						"m3",
						"n1",
						"n2",
						"n3",
						"o1",
						"o2",
						"o3",
						"o4",
						"l1e-a",
						"l1e",
						"l2e",
						"l3e",
						"l4e",
						"l5e",
						"l6e",
						"l7e"
					]
				},
				{
					"type": "null"
				}
			]
		},
		"techRecord_regnDate": {
			"anyOf": [
				{
					"type": "string",
					"pattern": "^$"
				},
				{
					"type": "string",
					"pattern": "^\\d{4}-\\d{2}-\\d{2}$"
				},
				{
					"type": "null"
				}
			]
		},
		"techRecord_manufactureYear": {
			"anyOf": [
				{
					"type": "integer",
					"minimum": 0,
					"maximum": 9999
				},
				{
					"type": "null"
				}
			]
		},
		"techRecord_noOfAxles": {
			"anyOf": [
				{
					"type": "integer",
					"minimum": 0,
					"maximum": 99
				},
				{
					"type": "null"
				}
			]
		},
		"techRecord_departmentalVehicleMarker": {
			"type": [
				"boolean",
				"null"
			]
		},
		"techRecord_alterationMarker": {
			"type": [
				"boolean",
				"null"
			]
		},
		"techRecord_approvalType": {
			"anyOf": [
				{
					"title": "Approval Type",
					"type": "string",
					"enum": [
						"NTA",
						"ECTA",
						"IVA",
						"NSSTA",
						"ECSSTA",
						"GB WVTA",
						"UKNI WVTA",
						"EU WVTA Pre 23",
						"EU WVTA 23 on",
						"QNIG",
						"Prov.GB WVTA",
						"Small series",
						"IVA - VCA",
						"IVA - DVSA/NI"
					]
				},
				{
					"type": "null"
				}
			]
		},
		"techRecord_approvalTypeNumber": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 25
		},
		"techRecord_ntaNumber": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 40
		},
		"techRecord_variantNumber": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 25
		},
		"techRecord_variantVersionNumber": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 35
		},
		"techRecord_bodyType_description": {
			"title": "Body Type Description",
			"type": "string",
			"enum": [
				"artic",
				"articulated",
				"box",
				"car transporter",
				"concrete mixer",
				"curtainsider",
				"double decker",
				"flat",
				"livestock carrier",
				"low loader",
				"mini bus",
				"other",
				"other tanker",
				"petrol/oil tanker",
				"refrigerated",
				"refuse",
				"single decker",
				"skeletal",
				"skip loader",
				"tipper",
				"tractor"
			]
		},
		"techRecord_bodyType_code": {
			"type": "string"
		},
		"techRecord_functionCode": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 1
		},
		"techRecord_conversionRefNumber": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 10
		},
		"techRecord_grossGbWeight": {
			"type": [
				"integer",
				"null"
			],
			"max": 99999,
			"min": 0
		},
		"techRecord_grossDesignWeight": {
			"type": [
				"integer",
				"null"
			],
			"max": 99999,
			"min": 0
		},
		"techRecord_createdByName": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_createdById": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_lastUpdatedByName": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_lastUpdatedById": {
			"type": [
				"string",
				"null"
			]
		}
	}
}