{
	"title": "Tech Record Search Skeleton Schema",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"techRecord_manufactureYear",
		"primaryVrm",
		"vin",
		"techRecord_statusCode",
		"techRecord_vehicleType",
		"createdTimestamp",
		"systemNumber"
	],
	"properties": {
		"techRecord_manufactureYear": {
			"type": [
				"integer",
				"null"
			]
		},
		"primaryVrm": {
			"type": "string"
		},
		"vin": {
			"type": "string"
		},
		"techRecord_statusCode": {
			"title": "Status Code",
			"type": "string",
			"enum": [
				"provisional",
				"current",
				"archived"
			]
		},
		"techRecord_vehicleType": {
			"title": "Vehicle Type",
			"type": "string",
			"enum": [
				"psv",
				"trl",
				"hgv",
				"car",
				"lgv",
				"motorcycle"
			]
		},
		"createdTimestamp": {
			"type": "string"
		},
		"systemNumber": {
			"type": "string"
		}
	}
}