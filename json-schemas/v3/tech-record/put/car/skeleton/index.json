{
	"title": "Tech Record PUT Request Skeleton Car Schema",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"vin",
		"techRecord_vehicleType"
	],
	"properties": {
		"vin": {
			"type": "string"
		},
		"primaryVrm": {
			"type": [
				"null",
				"string"
			]
		},
		"trailerId": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_reasonForCreation": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_vehicleType": {
			"title": "Vehicle Type",
			"type": "string",
			"enum": [
				"psv",
				"trl",
				"hgv",
				"car",
				"lgv",
				"motorcycle"
			]
		},
		"techRecord_statusCode": {
			"anyOf": [
				{
					"type": "null"
				},
				{
					"title": "Status Code",
					"type": "string",
					"enum": [
						"provisional",
						"current",
						"archived"
					]
				}
			]
		},
		"techRecord_regnDate": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_manufactureYear": {
			"type": [
				"string",
				"null"
			]
		},
		"techRecord_noOfAxles": {
			"type": [
				"integer",
				"null"
			]
		},
		"techRecord_notes": {
			"type": [
				"null",
				"string"
			]
		},
		"techRecord_hiddenInVta": {
			"type": [
				"null",
				"boolean"
			]
		},
		"techRecord_updateType": {
			"type": [
				"null",
				"string"
			]
		},
		"techRecord_createdAt": {
			"type": [
				"null",
				"string"
			]
		},
		"techRecord_createdById": {
			"type": [
				"null",
				"string"
			]
		},
		"techRecord_createdByName": {
			"type": [
				"null",
				"string"
			]
		},
		"secondaryVrms": {
			"type": [
				"null",
				"array"
			],
			"items": {
				"type": "string"
			}
		}
	}
}