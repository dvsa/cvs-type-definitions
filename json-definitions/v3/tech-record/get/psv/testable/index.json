    "techRecord_createdAt": {
      "type": "string"
    },
    "techRecord_vehicleConfiguration": {
      "$ref": "../../../enums/vehicleConfiguration.ignore.json"
    },
    "techRecord_vehicleSize": {
      "anyOf": [
        { "$ref": "../../../enums/vehicleSize.ignore.json" },
        { "type": "null" }
