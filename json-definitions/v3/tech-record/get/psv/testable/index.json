{
  "title": "GET PSV Technical Record V3 Testable",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "../../../sections/brakes/psv-brakes.ignore.json"
    },
    {
      "$ref": "../../../sections/applicant-details/applicant-details.ignore.json"
    }
