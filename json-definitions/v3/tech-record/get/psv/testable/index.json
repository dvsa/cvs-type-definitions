      "type": ["string", "null"],
      "maxLength": 250
    },
    "techRecord_dda_minEmergencyExits": {
      "type": ["integer", "null"],
      "minimum": 0,
      "maximum": 99
    },
    "techRecord_dda_outswing": {
      "type": ["string", "null"],
      "maxLength": 250
