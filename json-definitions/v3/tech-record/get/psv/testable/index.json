    "techRecord_regnDate": {
      "anyOf": [
        { "type": "string", "pattern": "^$" },
        { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
        { "type": "null" }
      ]
    },
    "techRecord_manufactureYear": {
      "anyOf": [
        { "type": "integer", "minimum": 0, "maximum": 9999 },
        { "type": "null" }
      ]
    },
    "techRecord_noOfAxles": {
      "anyOf": [
        { "type": "integer", "minimum": 0, "maximum": 99 },
        { "type": "null" }
      ]
