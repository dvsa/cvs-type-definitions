{
  "title": "GET PSV Technical Record V3 Skeleton",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "vin",
    "systemNumber",
    "primaryVrm",
    "vehicleType",
    "techRecord_vehicleConfiguration",
    "techRecord_vehicleSize",
    "techRecord_seatsLowerDeck",
    "techRecord_seatsUpperDeck",
    "techRecord_numberOfWheelsDriven",
    "techRecord_vehicleType",
    "techRecord_noOfAxles",
    "techRecord_statusCode",
    "techRecord_reasonForCreation",
    "techRecord_vehicleClass_code",
    "techRecord_vehicleClass_description",
    "techRecord_createdAt"
  ],
  "properties": {
    "vin": {
      "type": "string"
    },
    "systemNumber": {
      "type": "string"
    },
    "primaryVrm": {
      "type": "string"
    },
    "techRecord_vehicleType": {
      "const": "psv"
    },
    "techRecord_statusCode": {
      "$ref": "../../../enums/statusCode.ignore.json"
    },
    "techRecord_reasonForCreation": {
      "type": "string"
    },
    "techRecord_createdAt": {
      "type": "string"
    },
    "techRecord_vehicleConfiguration": {
      "$ref": "../../../enums/vehicleConfiguration.ignore.json"
    },
    "techRecord_vehicleSize": {
      "$ref": "../../../enums/vehicleSize.ignore.json"
    },
    "techRecord_seatsLowerDeck": {
      "type": "integer"
    },
    "techRecord_seatsUpperDeck": {
      "type": "integer"
    },
    "techRecord_numberOfWheelsDriven": {
      "type": ["string", "null"]
    },
    "techRecord_vehicleClass_code": {
      "type": "string"
    },
    "techRecord_vehicleClass_description": {
      "$ref": "../../../enums/vehicleClassDescription.ignore.json"
    },
    "techRecord_hiddenInVta": {
      "type": "boolean"
    },
    "techRecord_recordCompleteness": {
      "type": "string"
    },
    "techRecord_euVehicleCategory": {
      "anyOf": [
        { "$ref": "../../../enums/euVehicleCategory.ignore.json" },
        { "type": "null" }
      ]
    },
    "techRecord_regnDate": {
      "anyOf": [
        { "type": "string", "pattern": "^$" },
        { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
        { "type": "null" }
      ]
    },
    "techRecord_manufactureYear": {
      "anyOf": [
        { "type": "integer", "minimum": 0, "maximum": 9999 },
        { "type": "null" }
      ]
    },
    "techRecord_noOfAxles": {
      "anyOf": [
        { "type": "integer", "minimum": 0, "maximum": 99 },
        { "type": "null" }
      ]
    },
    "techRecord_departmentalVehicleMarker": {
      "type": ["boolean", "null"]
    },
    "techRecord_alterationMarker": {
      "type": ["boolean", "null"]
    },
    "techRecord_approvalType": {
      "anyOf": [
        { "$ref": "../../../enums/approvalType.ignore.json" },
        { "type": "null" }
      ]
    },
    "techRecord_approvalTypeNumber": {
      "type": ["string", "null"],
      "maxLength": 25
    },
    "techRecord_ntaNumber": {
      "type": ["string", "null"],
      "maxLength": 40
    },
    "techRecord_variantNumber": {
      "type": ["string", "null"],
      "maxLength": 25
    },
    "techRecord_variantVersionNumber": {
      "type": ["string", "null"],
      "maxLength": 35
    },
    "techRecord_bodyType_description": {
      "$ref": "../../../enums/bodyTypeDescription.ignore.json"
    },
    "techRecord_bodyType_code": {
      "type": "string"
    },
    "techRecord_functionCode": {
      "type": ["string", "null"],
      "maxLength": 1
    },
    "techRecord_conversionRefNumber": {
      "type": ["string", "null"],
      "maxLength": 10
    },
    "techRecord_grossGbWeight": {
      "type": ["integer", "null"],
      "max": 99999,
      "min": 0
    },
    "techRecord_grossDesignWeight": {
      "type": ["integer", "null"],
      "max": 99999,
      "min": 0
    },
    "techRecord_createdByName": {
      "type": ["string", "null"]
    },
    "techRecord_createdById": {
      "type": ["string", "null"]
    },
    "techRecord_lastUpdatedByName": {
      "type": ["string", "null"]
    },
    "techRecord_lastUpdatedById": {
      "type": ["string", "null"]
    }
  }
}
